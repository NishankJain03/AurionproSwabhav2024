package com.aurionpro.test;

import java.io.File;
import java.nio.file.Files;
import java.util.stream.Stream;

public class FileReaderStreamDemo {

	public static void main(String[] args) {
		String fileName = "D:\\Training\\Assignments\\FileReadAssignmnetUsingStream\\src\\test";
		
		File file = new File(fileName);
		
		Stream<String> linesStream = (Stream<String>) Files.readAllLines(file.toPath()){
			linesStream.forEach((line) -> System.out.println(line););
		}
		

	}

}
