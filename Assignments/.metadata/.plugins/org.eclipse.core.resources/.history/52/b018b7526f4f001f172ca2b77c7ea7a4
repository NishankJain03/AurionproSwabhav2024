package com.aurionpro.model;

public class Rectangle implements Shape{
	private String color;
	
	
	public Rectangle(String color) {
		super();
		this.color = color;
	}

	@Override
	protected Rectangle clone() {
		return super.clone();
	}
	
	@Override
	public void draw() {
		System.out.println("Drawing a " + color + " rectangle");
		
	}
	
}
