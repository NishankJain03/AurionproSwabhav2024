package com.aurionpro.test;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class StreamAssignment {

	public static void main(String[] args) {
		
		List<String> strings = Arrays.asList("Jayesh", "Nimesh", "Mark", "Mahesh" , "Ramesh");
		
		//sort in ascending order first 3 student
		
		List<String> ascString =  strings.stream().sorted().limit(3).collect(Collectors.toList());
		ascString.stream().forEach((string) -> System.out.println(string));
		

	}

}
